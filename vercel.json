{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/ghapi", 
            "destination": "https://api.github.com"
        },
        {
            "source": "/ghapi/:path*",
            "destination": "https://api.github.com/:path*"
        }
    ]
}